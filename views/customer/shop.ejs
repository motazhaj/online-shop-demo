<%- include('../shared/includes/head', { title: "Online Shop Demo" }) %>
    
<link rel="stylesheet" href="/styles/product.css">
</head>
<body>
    <%- include("../shared/includes/navbar")  %>
    <section id="headline">
        <h1>Welcome to <span class="highlight-text-primary">Green House</span>  <span class="highlight-text-secondary"><%= locals.name %></span></h1>
    </section>
    <section>
        <ul id="products-list">
            <% for(const item of products) { %>
                <%- include("../shared/includes/product-item",{item: item}) %>
            <% } %>
        </ul>
    </section>
</body>
</html>